<http://semantic-machines.com/veda/veda-site-main>
  rdf:type owl:Ontology ;
  rdfs:label "Сайт Veda. Главная страница"@ru ;
  rdfs:label "Veda site. Main page"@en ;
#  owl:versionInfo "1.3" ;
  v-s:loadPriority 10 ;
.

### MAIN ###

site:Main
  rdf:type site:Page ;
  rdfs:label "Главная страница"@ru ;
  rdfs:label "Main page"@en ;
  v-ui:hasTemplate site:MainTemplate ;
  site:hasPage site:Platform ;
  site:hasPage site:Applications ;
  site:hasPage site:Services ;
  site:hasPage site:About ;
  site:hasPage site:VedaOnline ;
.

### MAIN TEMPLATE ####

site:MainTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон главной страницы"@ru ;
  rdfs:label "Main page template"@en ;
  v-ui:template """
<div>
  <div about="site:Platform" data-template="site:PlatformMainTemplate"></div>
  <div about="site:Applications" data-template="site:ApplicationsMainTemplate"></div>
  <div about="site:Services" data-template="site:ServicesMainTemplate"></div>
  <div about="site:About" data-template="site:AboutMainTemplate"></div>
  <div about="site:VedaOnline" data-template="site:VedaOnlineMainTemplate"></div>
</div>
  """ ;
.
