<http://semantic-machines.com/veda/veda-site-product>
  rdf:type owl:Ontology ;
  rdfs:label "Veda site. About"@en ;
  rdfs:label "Сайт Veda. О компании"@ru ;
#  owl:versionInfo "1.3" ;
  v-s:loadPriority 10 ;
.

### SERVICES PAGE ###

site:About
  rdf:type site:Page ;
  rdfs:label "О компании"@ru ;
  rdfs:label "About us"@en ;
  v-ui:hasCustomTemplate site:AboutTemplate ;
  site:hasPage site:Customers ;
  site:hasPage site:Partners ;
.

### SERVICES PAGE TEMPLATE ####

site:AboutTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон страницы о компании"@ru ;
  rdfs:label "About page template"@en ;
  v-ui:template """
<div class="page container sheet">
  <div class="row">
    <div class="col-md-2 col-sm-4">
      <ul class="list-group" rel="site:hasPage">
        <a class="list-group-item" about="@" property="rdfs:label" href="#/@"></a>
      </ul>
    </div>
    <div class="col-md-10 col-sm-8">
      Содержание
    </div>
  </div>
</div>
  """ ;
.

site:Customers
  rdf:type site:Page ;
  rdfs:label "Клиенты"@ru ;
  rdfs:label "Customers"@en ;
.

site:Partners
  rdf:type site:Page ;
  rdfs:label "Партнерам"@ru ;
  rdfs:label "Partnership"@en ;
.
