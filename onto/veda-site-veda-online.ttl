### VEDA ONLINE ###

site:VedaOnline
  rdf:type site:Page ;
  rdfs:label "Veda онлайн"@ru ;
  rdfs:label "Veda online"@en ;
  v-ui:hasTemplate site:VedaOnlineTemplate ;
.

### VEDA ONLINE TEMPLATES ####

site:VedaOnlineMainTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон страницы Veda онлайн"@ru ;
  rdfs:label "Veda online page template"@en ;
  v-ui:template """
<div class="container-fluid bg-peter-river">
  <div class="container">
    <h1 about="@" property="rdfs:label"></h1>
  </div>
</div>
  """ ;
.

site:VedaOnlineTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон страницы Veda онлайн"@ru ;
  rdfs:label "Veda online page template"@en ;
  v-ui:template """
<div style="background-color: #fff;">
  <div style="background-image: url('/files/site:ImageBackForOnline'); background-size: cover; background-position: center center; height:350px;">
    <div class="container">
      <br>
      <br>
      <h1 style="line-height: 1; color: #fff; text-shadow: 0px 1px 2px black;">
        <span about="site:ApplicationsSlogan1" property="rdfs:label"></span>
        <br>
        <small about="site:ApplicationsSlogan1" property="rdfs:comment" style="color: #fff;"></small>
      </h1>
      <br>
      <br>
      <br>
      <br>
      <br>
      <br>
      <br>
      <br>
    </div>
  </div>
  <br>
  <div class="container">
    <div>
      <blockquote>
        <p class="lead" about="site:ApplicationsSlogan2" property="rdfs:comment"></p>
      </blockquote>
      <div class="row">
        <div class="col-md-4">
          <div class="panel panel-default" style="border: none; background-image: url('/files/site:ImageBackForOnline1'); background-size: cover; background-position: center center; height:250px;">
            <a href="#/site:ApplicationsContractors" style="text-decoration: none;">
              <div class="panel-header">
              </div>
              <div class="panel-body">
                <h3 about="site:ApplicationsContractors" property="rdfs:label" style="color: #fff;"></h3>
                <span style="color: #fff;">
                  Приложение предназначено для централизации работы и детального анализа данных по каждому отдельному контрагенту,
                  проведения формализованной процедуры проверки соответствия поставщиков внутренним требованиям организации.
                </span>
              </div>
            </a>
          </div>
          <br>
          <div class="panel panel-default" style="border: none; background-image: url('/files/site:ImageBackForOnline2'); background-size: cover; background-position: center center; height:250px;">
            <a href="#/site:ApplicationsMeetings" style="text-decoration: none;">
              <div class="panel-header">
              </div>
              <div class="panel-body">
                <h3 about="site:ApplicationsMeetings" property="rdfs:label"></h3>
                <span style="color: #000;">
                  Приложение предназначено для автоматизации планирования и проведения различных встреч (совещаний, заседаний, рабочих встреч...),
                  контроля  выполнения планов реализации.
                </span>
              </div>
            </a>
          </div>
        </div>
        <div class="col-md-4">
          <div class="panel panel-default" style="border: none; background-image: url('/files/site:ImageBackForOnline2'); background-size: cover; background-position: center center; height:250px;">
            <a href="#/site:ApplicationsContracts" style="text-decoration: none;">
              <div class="panel-header">
              </div>
              <div class="panel-body">
                <h3 about="site:ApplicationsContracts" property="rdfs:label"></h3>
                <span style="color: #000;">
                  Приложение предназначено для обеспечения контроля и прозрачности договорных процессов,
                  централизации работы по каждому отдельному контракту и связанными с ним документами.
                  Включает в себя  автоматизацию полного цикла договорной работы.
                </span>
              </div>
            </a>
          </div>
          <br>
          <div class="panel panel-default" style="border: none; background-image: url('/files/site:ImageBackForOnline1'); background-size: cover; background-position: center center; height:250px;">
            <a href="#/site:ApplicationsAudits" style="text-decoration: none;">
              <div class="panel-header">
              </div>
              <div class="panel-body">
                <h3 about="site:ApplicationsAudits" property="rdfs:label" style="color: #fff;"></h3>
                <span style="color: #fff;">
                  Приложение предназначено для автоматизации планирования и проведения аудитов, контроля выполнения планов реализации.
                </span>
              </div>
            </a>
          </div>
        </div>
        <div class="col-md-4">
          <div class="panel panel-default" style="border: none; background-image: url('/files/site:ImageBackForOnline1'); background-size: cover; background-position: center center; height:250px;">
            <a href="#/site:ApplicationsAccountingDocs" style="text-decoration: none;">
              <div class="panel-header">
              </div>
              <div class="panel-body">
                <h3 about="site:ApplicationsAccountingDocs" property="rdfs:label" style="color: #fff;"></h3>
                <span style="color: #fff;">
                  Приложение предназначено для обеспечения оперативной обработки и хранения первичной документации.
                </span>
              </div>
            </a>
          </div>
          <br>
          <div class="panel panel-default" style="border: none; background-image: url('/files/site:ImageBackForOnline2'); background-size: cover; background-position: center center; height:250px;">
            <a href="#/site:ApplicationsAudits" style="text-decoration: none;">
              <div class="panel-header">
              </div>
              <div class="panel-body">
                <h3 about="site:ApplicationsAudits" property="rdfs:label"></h3>
                <span style="color: #000;">
                  Пустой блок
                </span>
              </div>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
  """ ;
.

